def pluralize(noun):
    if noun.endswith('y') and noun[-2] not in 'aeiou':
        return noun[:-1] + 'ies'
    elif noun.endswith(('s', 'x', 'z', 'ch', 'sh')):
        return noun + 'es'
    else:
        return noun + 's'

# Test nouns
nouns = ["cat", "dog", "baby", "box", "bus", "church"]

for n in nouns:
    print(n, "->", pluralize(n))
